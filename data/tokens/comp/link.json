{
  "link": {
    "text-content": {
      "sm": {
        "value": "{body.text-content.sm}",
        "type": "typography"
      },
      "md": {
        "value": "{body.text-content.md}",
        "type": "typography"
      },
      "lg": {
        "value": "{body.text-content.lg}",
        "type": "typography"
      }
    },
    "primary": {
      "text-color": {
        "value": "{semantic.color.primary}",
        "type": "color"
      },
      "border-color": {
        "value": "{semantic.color.primary}",
        "type": "color"
      }
    },
    "neutral": {
      "default": {
        "text-color": {
          "value": "{semantic.color.on-background}",
          "type": "color"
        },
        "border-color": {
          "value": "{semantic.color.on-background}",
          "type": "color",
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "lighten",
                "value": ".8",
                "space": "lch"
              }
            }
          }
        }
      },
      "hover": {
        "border-color": {
          "value": "{semantic.color.on-background}",
          "type": "color"
        }
      }
    },
    "border-width": {
      "sm": {
        "value": "{border-width.1}",
        "type": "borderWidth"
      }
    }
  },
  "navlink": {
    "default": {
      "text-color": {
        "value": "{semantic.color.on-surface}",
        "type": "color",
        "$extensions": {
          "studio.tokens": {
            "modify": {
              "type": "lighten",
              "value": ".8",
              "space": "lch"
            }
          }
        }
      },
      "border-color": {
        "value": "{semantic.color.primary}",
        "type": "color"
      }
    },
    "active": {
      "text-color": {
        "value": "{semantic.color.on-surface}",
        "type": "color"
      }
    },
    "text-content": {
      "active": {
        "md": {
          "value": {
            "fontFamily": "{semantic.font-family.typography.label}",
            "fontWeight": "{font-weight.bold}",
            "lineHeight": "{line-height.350}",
            "fontSize": "{semantic.font-size.1} * {semantic.font-nRatio.label}",
            "letterSpacing": "0%",
            "paragraphSpacing": "0",
            "textDecoration": "none",
            "textCase": "{text-case.uppercase}"
          },
          "type": "typography"
        }
      },
      "default": {
        "md": {
          "value": {
            "fontFamily": "{semantic.font-family.typography.label}",
            "fontWeight": "{semantic.font-weight.typography.label.semibold}",
            "lineHeight": "{line-height.350}",
            "fontSize": "{semantic.font-size.1} * {semantic.font-nRatio.label}",
            "letterSpacing": "0%",
            "paragraphSpacing": "0",
            "textDecoration": "none",
            "textCase": "{text-case.uppercase}"
          },
          "type": "typography"
        }
      }
    }
  }
}